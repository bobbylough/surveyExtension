{
  "manifest_version": 3,
  "name": "Survey MVP",
  "version": "1.0",
  "description": "An MVP version of a Chrome Extension to get survey questions from an API and submit answers",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "permissions": ["activeTab"],
  "host_permissions": ["https://mockbackend.vercel.app/*"],
  "icons": {
    "128": "icon-128.png"
  }
}